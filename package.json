{
  "name": "subscan-ui-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prettier": "prettier './src' --check",
    "prettier-fix": "prettier './src' --write",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-react-icon": "node ./src/ui/Svg/build.js react",
    "build-optimized-icon": "rimraf ./src/ui/Svg/optimized/* && svgo --config=./src/ui/Svg/svgo.config.js -f ./src/ui/Svg/svg/ -o ./src/ui/Svg/optimized --pretty --indent=2",
    "cloudflare-build": "npx @cloudflare/next-on-pages@1.0.0",
    "cloudflare-dev": "npx wrangler@3.0.0 pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src": [
      "lint-fix",
      "prettier-fix"
    ]
  },
  "dependencies": {
    "@floating-ui/react": "0.23.1",
    "@headlessui/react": "1.7.14",
    "@polkadot/api": "^10.5.1",
    "@polkadot/api-contract": "^10.5.1",
    "@polkadot/extension-dapp": "^0.46.2",
    "@polkadot/ui-keyring": "^3.2.2",
    "@polkadot/util": "^12.1.2",
    "@polkadot/util-crypto": "^12.1.2",
    "@polkadot/vue-identicon": "^3.2.2",
    "@polkassembly/util": "^0.2.0",
    "@types/node": "18.15.11",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "bignumber.js": "9.1.1",
    "clsx": "1.2.1",
    "date-fns-tz": "^2.0.0",
    "dayjs": "1.11.7",
    "echarts": "5.4.2",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "graphemer": "^1.4.0",
    "i18next": "^22.4.15",
    "next": "13.3.0",
    "next-i18next": "^13.2.2",
    "next-intl": "2.13.0",
    "nprogress": "^0.2.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "react-i18next": "^12.2.2",
    "styled-components": "6.0.0-beta.2",
    "swr": "2.1.5",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "usehooks-ts": "2.9.1",
    "xlsx": "^0.18.5",
    "@subscan/parachains-info": "^2.1.41"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "sass": "^1.62.0",
    "storybook": "^7.0.6",
    "svgo": "^3.0.2"
  }
}
